<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.liyingtao.bean.Jd" table="t_jd">
        <id name="jdId" type="java.lang.Integer" column="jdid">
            <generator class="native"/>
        </id>
        <property name="jdName" type="java.lang.String" column="jdname" >
        </property>
        <many-to-one name="qy" class="com.liyingtao.bean.Qy" column="qyid11" />
    </class>
</hibernate-mapping>